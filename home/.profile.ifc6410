  return 0 # Exit the script if already sourced
fi

# Set the guard variable
SOURCED_PROFILE_IFC6410=1

PROMPT='%n@%m:%~%# '

lsb_release -a
echo
uname -a
echo
echo "Welcome to $(hostname) SBC shell"
date
